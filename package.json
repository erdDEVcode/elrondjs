{
  "name": "elrondjs",
  "version": "1.2.1",
  "description": "Javascript SDK for the Elrond blockchain with Typescript support.",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "author": "erdDEV team (https://erd.dev)",
  "homepage": "https://elrondjs.erd.dev",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "package.json",
